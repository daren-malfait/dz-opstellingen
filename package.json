  "version": "0.0.0-semantically-released",
  "scripts": {
    "build": "cross-env SANITY_ACTIVE_ENV=production sanity build",
    "dev": "cross-env SANITY_ACTIVE_ENV=development sanity start",
    "deploy": "cross-env SANITY_ACTIVE_ENV=production sanity deploy",
    "export": "npx sanity dataset export production --no-drafts ./data/production.tar.gz",
    "import": "npx sanity dataset import ./data/production.tar.gz",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install",
    "test": "sanity check",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint --silent && npm run typecheck --silent"
  },
